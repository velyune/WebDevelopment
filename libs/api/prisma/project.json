{
  "name": "prisma",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/prisma/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "status": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate status",
        "cwd": "libs/api/prisma"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev",
        "cwd": "libs/api/prisma"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio",
        "cwd": "libs/api/prisma"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy",
        "cwd": "libs/api/prisma"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate",
        "cwd": "libs/api/prisma"
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db seed",
        "cwd": "libs/api/prisma"
      }
    }
  }
}
